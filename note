class test {
	num a;

	constructor(num b){
		a=b;
	}

	num foo(){ // num test_foo(self)
		a=7;		// self->a = 7;
	}
}

num main(){
	test t;
	t = new test(3);
	t.foo(); // test_foo(t);

}


// precedence in cdfuncs -> locals, formals, instance (self), globals
// method lifting cdfuncs -> make struct from cdvars return ptr -> add to formal parameter of func
// construct --> malloc room for vars and struct ptr
// classCall --> pass in struct ptr --> add to actuals 
